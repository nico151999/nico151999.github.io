{
	"name": "nico151999.github.io",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "pnpm run smui-theme-light && pnpm run smui-theme-dark && svelte-kit sync || echo ''",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme -i ./node_modules",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark -i ./node_modules",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"devDependencies": {
		"@bonosoft/sveltekit-timeline": "^0.0.15",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@fortawesome/free-brands-svg-icons": "^6.7.2",
		"@fortawesome/svelte-fontawesome": "^0.2.3",
		"@material/typography": "^14.0.0",
		"@mdi/js": "^7.4.47",
		"@smui/card": "^8.0.1",
		"@smui/common": "^8.0.1",
		"@smui/icon-button": "^8.0.1",
		"@smui/image-list": "^8.0.1",
		"@smui/layout-grid": "^8.0.1",
		"@smui/linear-progress": "^8.0.1",
		"@smui/list": "^8.0.1",
		"@smui/tooltip": "^8.0.1",
		"@smui/top-app-bar": "^8.0.1",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@vitest/browser": "^3.2.3",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"playwright": "^1.53.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"smui-theme": "^8.0.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
